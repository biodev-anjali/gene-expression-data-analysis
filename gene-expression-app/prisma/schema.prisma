datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model GeneExpressionRun {
  id        String   @id @default(uuid())
  fileName  String?
  fileHash  String   @unique
  genes     Int
  samples   Int
  meanExpr  Float?
  createdAt DateTime @default(now())
}
